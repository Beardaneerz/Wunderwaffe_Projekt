






function AI_INPUT.onKeyboardKeyDown(kKeyCode)




if(kKeyCode==25)
then
table.setAt(this.tInput(),0,true)
end

if(kKeyCode==3)
then
table.setAt(this.tInput(),1,true)
end

if(kKeyCode==18)
then
table.setAt(this.tInput(),2,true)
end

if(kKeyCode==16)
then
table.setAt(this.tInput(),3,true)
end





if(kKeyCode==7)
then
log.message("TIR")
this.bFire(true)
end


end
